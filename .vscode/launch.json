    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Scan",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "scan"
            ],
            "env": {
                "AZURE_SDK_GO_LOGGING": "all"
            }
        },
        {
            "name": "Launch Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "serve"
            ],
            "env": {
                "AZURE_SDK_GO_LOGGING": "all"
            }
        }
    ]
}