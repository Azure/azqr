// Check if AKS has outbound type set to user defined routing
resources
| where type =~ 'Microsoft.ContainerService/managedClusters'
| extend outboundType = properties.networkProfile.outboundType
| where isnull(outboundType) or outboundType != 'userDefinedRouting'
| extend recommendationId = 'aks-012'
| extend param1 = ''
| project recommendationId, name, id, tags, param1
